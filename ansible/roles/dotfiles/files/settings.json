{
    "workbench.startupEditor": "newUntitledFile",
    "workbench.settings.editor": "json",
    "editor.fontFamily": "Fira Code",
    "editor.fontLigatures": "'ss01', 'ss02', 'ss03', 'ss04', 'ss05', 'ss06', 'zero'",
    "editor.wordWrap": "on",
    "editor.scrollBeyondLastLine": false,
    "window.title": "${activeEditorMedium}${separator}${rootName}${dirty}",
    "terminal.integrated.fontFamily": "Source Code Pro",
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.cursorStyle": "line",
    "terminal.integrated.scrollback": 1000,
	"javascript.validate.enable": false,
    "css.validate": false,
    "scss.validate": false,
    "less.validate": false,
    "gitlens.statusBar.alignment": "left",
    "gitlens.statusBar.format": "${id} - ${author}, ${ago} - ${message|40}",
    "editor.insertSpaces": true,
    "editor.tabSize": 4,
    "terminal.integrated.confirmOnExit": true,
    "window.zoomLevel": 0,
    "workbench.colorTheme": "Noctis Obscuro",
    "bracketPairColorizer.timeOut": 0,
    "gitlens.blame.format": "${author|7} - ${message|20?} ${ago|14-}",
    "gitlens.hovers.currentLine.enabled": false,
    "gitlens.currentLine.enabled": false,
    "gitlens.hovers.currentLine.over": "line",
    "gitlens.keymap": "chorded",
    "auto-close-tag.activationOnLanguage": [
        "ejs",
        "HTML (Eex)",
        "javascript",
        "javascriptreact",
        "jinja",
        "markdown",
        "plaintext",
        "typescript",
        "typescriptreact",
        "vue",
        "xml"
    ],
    "gitlens.advanced.messages": {
        "suppressGitVersionWarning": true,
        "suppressShowKeyBindingsNotice": true
    },
    "workbench.editor.labelFormat": "short",
    "workbench.editor.enablePreview": false,
    "extensions.ignoreRecommendations": false,
    "editor.quickSuggestions": {
        "other": false,
        "comments": true,
        "strings": false
    },
    "editor.quickSuggestionsDelay": 10,
    "editor.wordBasedSuggestions": false,
    "editor.acceptSuggestionOnEnter": "off",
    "editor.acceptSuggestionOnCommitCharacter": false,
    "explorer.confirmDragAndDrop": false,
    "explorer.confirmDelete": false,
    "python.linting.pylintPath": "/usr/local/bin/pylint",
    "python.linting.enabled": true,
    "python.linting.pylintEnabled": true,
    "python.linting.pylintArgs": [
        "--load-plugins=pylint_django"
    ],
    "python.pythonPath": "/home/vagrant/.local/share/virtualenvs/aster-8hC4QV6C/bin/python",
    "python.envFile": "${workspaceFolder}/.env",
    "python.jediEnabled": true,
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "workbench.iconTheme": "material-icon-theme",
    "search.exclude": {
        "**/build": true,
        "**/vendor": true,
        "**/node_modules": true,
        "**/public": true,
        "**/.git": true,
        ".vscode": true,
        "bazel-*/**/*.*": true
    },
    "html.format.endWithNewline": true,
    "html.format.indentInnerHtml": true,
    "html.format.wrapLineLength": 0,
    "gitlens.views.fileHistory.enabled": true,
    "gitlens.views.lineHistory.enabled": true,
    "files.trimTrailingWhitespace": true,
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "git.ignoreLegacyWarning": true,
    "workbench.colorCustomizations": {
        "editor.selectionBackground": "#135564",
        "editor.selectionHighlightBackground": "#135564"
    },
    "typescript.updateImportsOnFileMove.enabled": "prompt",
    "files.watcherExclude": {
        "**/.git/**": true,
        "**/*.pyc": true,
        "**/node_modules/**": true,
        "**/bazel-beeswaxio": true,
        "**/bazel-bin": true,
        "**/bazel-out": true,
        "**/bazel-testlogs": true,
        "**/bazel-genfiles/**": true,
    },
    "files.exclude": {
        "**/.git/**": true,
        "**/*.pyc": true,
        "bazel-*/**/*.*": true
    },
    "eslint.alwaysShowStatus": true,
    "intelephense.environment.documentRoot": "./beeswax/buzz_api/buzz/api/api.php",
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "vsintellicode.python.completionsEnabled": false,
    "remote.SSH.defaultExtensions": [
        "alefragnani.bookmarks",
        "batisteo.vscode-django",
        "bazelbuild.vscode-bazel",
        "bmewburn.vscode-intelephense-client",
        "dbaeumer.vscode-eslint",
        "eamodio.gitlens",
        "editorconfig.editorconfig",
        "felixfbecker.php-debug",
        "hookyqr.beautify",
        "ms-azuretools.vscode-docker",
        "ms-python.python",
        "tyriar.sort-lines",
        "visualstudioexptteam.vscodeintellicode",
        "ziyasal.vscode-open-in-github",
        "zxh404.vscode-proto3"
    ],
    "remote.SSH.enableDynamicForwarding": false,
    "intelephense.diagnostics.run": "onSave",
    "intelephense.environment.phpVersion": "7.4.1",
    "terminal.integrated.shell.linux": "/bin/zsh",
    "python.testing.pytestEnabled": true,
    "latex-workshop.view.pdf.viewer": "external"
}
