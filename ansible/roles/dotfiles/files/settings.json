{
  "workbench.settings.editor": "json",
  "editor.fontFamily": "Fira Code",
  "editor.fontLigatures": "'ss01', 'ss02', 'ss03', 'ss04', 'ss05', 'ss06', 'zero'",
  "editor.wordWrap": "on",
  "editor.scrollBeyondLastLine": false,
  "editor.minimap.showSlider": "always",
  "window.title": "${activeEditorMedium}${separator}${rootName}${dirty}",
  "terminal.integrated.fontFamily": "Fira Code",
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.scrollback": 1000,
  "javascript.validate.enable": false,
  "css.validate": false,
  "scss.validate": false,
  "less.validate": false,
  "gitlens.statusBar.alignment": "left",
  "gitlens.statusBar.format": "${id} - ${author}, ${ago} - ${message|40}",
  "terminal.integrated.confirmOnExit": "never",
  "workbench.colorTheme": "Noctis Obscuro",
  "gitlens.blame.format": "${author|7} - ${message|20?} ${ago|14-}",
  "gitlens.hovers.currentLine.enabled": false,
  "gitlens.currentLine.enabled": false,
  "gitlens.hovers.currentLine.over": "line",
  "gitlens.keymap": "chorded",
  "gitlens.advanced.messages": {
    "suppressGitVersionWarning": true
  },
  "workbench.editor.labelFormat": "short",
  "workbench.editor.enablePreview": false,
  "extensions.ignoreRecommendations": false,
  "editor.quickSuggestions": {
    "other": false,
    "comments": true,
    "strings": false
  },
  "editor.quickSuggestionsDelay": 10,
  "editor.wordBasedSuggestions": false,
  "editor.acceptSuggestionOnEnter": "off",
  "editor.acceptSuggestionOnCommitCharacter": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmDelete": false,
  "python.formatting.provider": "black",
  "python.formatting.blackPath": "~/.local/bin/black",
  "python.linting.pylintPath": "~/.local/lib/beeswax/py3_pylint_venv/bin/pylint",
  "python.linting.enabled": true,
  "python.linting.pylintEnabled": true,
  "python.linting.pylintArgs": [
    "--rcfile=${env:HOME}/beeswaxio/beeswax/buzz_next/.pylintrc"
  ],
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "workbench.iconTheme": "material-icon-theme",
  "search.exclude": {
    "**/build": true,
    "**/vendor": true,
    "**/node_modules": true,
    "**/public": true,
    "**/.git": true,
    ".vscode": true,
    "bazel-*/**/*.*": true
  },
  "html.format.endWithNewline": true,
  "html.format.indentInnerHtml": true,
  "html.format.wrapLineLength": 0,
  "files.trimTrailingWhitespace": true,
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "git.ignoreLegacyWarning": true,
  "git.detectSubmodulesLimit": 15,
  "workbench.colorCustomizations": {
    "editor.selectionBackground": "#135564",
    "editor.selectionHighlightBackground": "#135564"
  },
  "typescript.updateImportsOnFileMove.enabled": "prompt",
  "files.watcherExclude": {
    "**/.git/**": true,
    "**/*.pyc": true,
    "**/node_modules/**": true,
    "**/bazel-beeswaxio": true,
    "**/bazel-bin": true,
    "**/bazel-out": true,
    "**/bazel-testlogs": true,
    "**/bazel-genfiles/**": true,
  },
  "files.exclude": {
    "**/.git/**": true,
    "**/*.pyc": true,
    "bazel-*/**/*.*": true
  },
  "intelephense.environment.documentRoot": "./beeswax/buzz_api/buzz/api/api.php",
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "vsintellicode.python.completionsEnabled": false,
  "remote.SSH.defaultExtensions": [
    "alefragnani.bookmarks",
    "batisteo.vscode-django",
    "bazelbuild.vscode-bazel",
    "bmewburn.vscode-intelephense-client",
    "dbaeumer.vscode-eslint",
    "eamodio.gitlens",
    "editorconfig.editorconfig",
    "felixfbecker.php-debug",
    "hookyqr.beautify",
    "ms-azuretools.vscode-docker",
    "ms-python.python",
    "tyriar.sort-lines",
    "visualstudioexptteam.vscodeintellicode",
    "ziyasal.vscode-open-in-github",
    "zxh404.vscode-proto3"
  ],
  "remote.SSH.enableDynamicForwarding": false,
  "intelephense.diagnostics.run": "onSave",
  "intelephense.environment.phpVersion": "7.4.1",
  "python.testing.pytestEnabled": false,
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "[python]": {
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.organizeImports": true
    }
  },
  "python.languageServer": "Pylance",
  "python.showStartPage": false,
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "workbench.startupEditor": "none",
  "[sql]": {
    "editor.defaultFormatter": "mtxr.sqltools"
  },
  "docker.showStartPage": false,
  "editor.bracketPairColorization.enabled": true
}
